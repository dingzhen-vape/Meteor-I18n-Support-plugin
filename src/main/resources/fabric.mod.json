{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Meteor I18n Support",
  "description": "为Meteor添加国际化支持",
  "authors": [
    "kono_yalu"
  ],
  "contact": {
    "sources": "https://github.com/dingzhen-vape/meteor-addon-Translate"
  },
  "icon": "assets/yalu/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.yalu.addon.TranslateAddon"
    ]
  },
  "mixins": [
    "addon-Translate.mixins.json"
  ],
